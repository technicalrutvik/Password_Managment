<%- include('header') %>


    <div class="container">

      <div class="row">

          <h1>Password Catagory Lists</h1>
          <table class="table table-striped text-primary">
            <thead>
              <tr>
                 
                <th>Password Catagory Name</th>

                <th colspan="2">Action</th>
       
              </tr>
            </thead>
        <tbody>

           <% if(records.length>0) { 
             records.forEach(function(row){
              %>
            <tr>   
              <td><%= row.password_category %></td>

              <td><a class="btn btn-primary btn-xs" href="/password_catagory/edit/<%= row._id %>"><i class="fa fa-edit"></i></a>
               <a class="btn btn-danger btn-xs" href="/password_catagory/delete/<%= row._id %>"><i class="fa fa-trash"></i></a>
             </td>
            </tr>
           <% }) } %>  
           

        </tbody>
         
        </table>
       </div>
       </div>
       <%- include('footer') %>

